---
layout: feed
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Brian Tom's Blog</title>
	<link href="https://btx123.github.io/blog/atom.xml" rel="self" />
	<link href="https://btx123.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://btx123.github.io/blog</id>
	<author>
		<name>Brian Tom</name>
		<email>tombl@uci.edu</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://btx123.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://btx123.github.io/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>